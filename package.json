{
  "name": "framefor",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "app",
    "mobile"
  ],
  "scripts": {
    "dev:web": "cd app && npm run dev",
    "dev:mobile": "cd mobile && npm start",
    "build:web": "cd app && npm run build",
    "db:generate": "cd app && npx prisma generate",
    "db:push": "cd app && npx prisma db push",
    "db:studio": "cd app && npx prisma studio"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.14"
  }
}
